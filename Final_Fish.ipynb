    "US_map.plot(ax = ax, color = \"grey\", edgecolor = \"black\", legend = True)\n",
    "plt.xlim(-180, -50)\n",
    "plt.ylim(15, 75)\n",
    "plt.xlabel(\"longitude\", fontsize = 15)\n",
